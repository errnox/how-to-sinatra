%div.container
  %div.col-md-12
    %h2 Session
    - if @user_name != nil && @user_name != ''
      %p
        Username:
        %strong= @user_name
    - else
      %p
        %em No username provided
    %pre= session.inspect
    %div.container
      %div.col-md-2.col-md-offset-3
        %a.btn.btn-default{:href => '/session?user_name=john'} Set username
      %div.col-md-2.col-md-offset-2
        %a.btn.btn-default{:href => '/session'} Unset username
